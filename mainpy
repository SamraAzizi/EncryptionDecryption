from tkinter import *
from tkinter import messagebox
import base64
import os

def main_screen():
    screen = Tk()
    screen.geometry("375x390")

    #icon
    image_icon = PhotoImage(file="keys.png")
    screen.iconphoto(False, image_icon)

    screen.title("Encrypt and Decrypt")

    screen.mainloop()


main_screen()